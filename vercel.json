{
  "version": 2,
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/models/(.*)",
      "dest": "/models/$1"
    },
    {
      "src": "^/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "^/animations/(.*)",
      "dest": "/animations/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/logo(.*).png",
      "dest": "/logo$1.png"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
} 